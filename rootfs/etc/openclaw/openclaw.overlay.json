{
  "env": {
    "vars": {
      "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}",
      "OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}",
      "OPENAI_API_KEY": "${OPENAI_API_KEY}",
      "MEETING_CHIEF_DB": "${MEETING_CHIEF_DB}"
    }
  },
  "diagnostics": {
    "enabled": true,
    "cacheTrace": {
      "enabled": true,
      "includeMessages": false,
      "includePrompt": false,
      "includeSystem": false
    }
  },
  "logging": {
    "level": "info",
    "redactSensitive": "tools"
  },
  "browser": {
    "enabled": true,
    "headless": true,
    "noSandbox": true
  },
  "auth": {
    "profiles": {
      "anthropic:default": {
        "provider": "anthropic",
        "mode": "api_key"
      },
      "openrouter:default": {
        "provider": "openrouter",
        "mode": "api_key"
      }
    }
  },
  "models": {
    "providers": {
      "gradient": {
        "models": [
          {
            "id": "anthropic-claude-opus-4.6",
            "name": "Claude Opus 4.6",
            "reasoning": true,
            "input": [
              "text",
              "image"
            ],
            "contextWindow": 1000000,
            "maxTokens": 128000
          },
          {
            "id": "anthropic-claude-opus-4.5",
            "name": "Claude Opus 4.5",
            "reasoning": true,
            "input": [
              "text",
              "image"
            ],
            "contextWindow": 1000000,
            "maxTokens": 64000
          },
          {
            "id": "anthropic-claude-4.5-sonnet",
            "name": "Claude Sonnet 4.5",
            "reasoning": false,
            "input": [
              "text",
              "image"
            ],
            "contextWindow": 1000000,
            "maxTokens": 64000
          },
          {
            "id": "anthropic-claude-sonnet-4",
            "name": "Claude Sonnet 4",
            "reasoning": false,
            "input": [
              "text",
              "image"
            ],
            "contextWindow": 1000000,
            "maxTokens": 64000
          },
          {
            "id": "anthropic-claude-4.5-haiku",
            "name": "Claude Haiku 4.5",
            "reasoning": false,
            "input": [
              "text",
              "image"
            ],
            "contextWindow": 1000000,
            "maxTokens": 64000
          },
          {
            "id": "llama3.3-70b-instruct",
            "name": "Llama 3.3 70B Instruct",
            "reasoning": false,
            "input": [
              "text"
            ],
            "contextWindow": 128000,
            "maxTokens": 8192
          },
          {
            "id": "deepseek-r1-distill-llama-70b",
            "name": "DeepSeek R1 Distill Llama 70B",
            "reasoning": true,
            "input": [
              "text"
            ],
            "contextWindow": 128000,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "gradient/anthropic-claude-opus-4.6",
        "fallbacks": [
          "anthropic/claude-opus-4-6",
          "openrouter/openrouter/auto"
        ]
      },
      "workspace": "/data/.openclaw/workspace",
      "memorySearch": {
        "enabled": true,
        "sources": [
          "memory"
        ],
        "extraPaths": [
          "/data/.openclaw/workspace/memory/meetings"
        ],
        "provider": "local",
        "fallback": "openai",
        "query": {
          "maxResults": 10,
          "minScore": 0.3,
          "hybrid": {
            "enabled": true,
            "vectorWeight": 0.7,
            "textWeight": 0.3
          }
        }
      },
      "contextPruning": {
        "mode": "cache-ttl",
        "ttl": "2h"
      },
      "compaction": {
        "mode": "default"
      },
      "thinkingDefault": "low",
      "heartbeat": {
        "every": "30m"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8,
        "model": "gradient/anthropic-claude-sonnet-4"
      },
      "sandbox": {
        "mode": "off",
        "workspaceAccess": "rw"
      }
    }
  },
  "tools": {
    "media": {
      "audio": {
        "enabled": true
      }
    },
    "agentToAgent": {
      "enabled": true
    }
  },
  "messages": {
    "ackReactionScope": "direct"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto",
    "restart": true
  },
  "approvals": {
    "exec": {
      "enabled": true,
      "mode": "targets",
      "targets": [
        {
          "channel": "whatsapp",
          "to": "+14124177553"
        }
      ]
    }
  },
  "session": {
    "dmScope": "main",
    "resetTriggers": [
      "/new",
      "/reset"
    ],
    "reset": {
      "mode": "daily",
      "atHour": 4,
      "idleMinutes": 1440
    }
  },
  "hooks": {
    "enabled": true,
    "token": "${HOOKS_TOKEN}",
    "internal": {
      "enabled": true,
      "entries": {
        "boot-md": {
          "enabled": true
        },
        "command-logger": {
          "enabled": true
        },
        "session-memory": {
          "enabled": true
        }
      }
    }
  },
  "channels": {
    "whatsapp": {
      "dmPolicy": "allowlist",
      "allowFrom": [
        "+14124177553"
      ],
      "groupPolicy": "allowlist",
      "mediaMaxMb": 50,
      "debounceMs": 1500
    }
  },
  "canvasHost": {
    "enabled": true,
    "liveReload": true
  },
  "talk": {
    "apiKey": "${OPENAI_REALTIME_API_KEY}",
    "interruptOnSpeech": true
  },
  "plugins": {
    "allow": [
      "whatsapp",
      "memory-core"
    ],
    "entries": {
      "memory-core": {
        "enabled": true
      }
    }
  },
  "memory": {
    "backend": "builtin",
    "citations": "auto",
    "qmd": {
      "includeDefaultMemory": true,
      "paths": [
        {
          "path": "/data/.openclaw/workspace/memory/meetings",
          "name": "meeting-summaries",
          "pattern": "**/*.md"
        },
        {
          "path": "/data/.openclaw/workspace/skills",
          "name": "skills",
          "pattern": "**/SKILL.md"
        }
      ],
      "update": {
        "interval": "10m",
        "debounceMs": 15000,
        "onBoot": true
      },
      "limits": {
        "maxResults": 8,
        "maxSnippetChars": 800,
        "timeoutMs": 5000
      }
    }
  },
  "skills": {
    "install": {
      "preferBrew": false,
      "nodeManager": "pnpm"
    },
    "entries": {
      "asana": {
        "enabled": true,
        "apiKey": "${ASANA_API_KEY}"
      },
      "clickup-mcp": {
        "enabled": true,
        "env": {
          "CLICKUP_TOKEN": "${CLICKUP_TOKEN}"
        }
      },
      "google-workspace": {
        "enabled": true
      },
      "goplaces": {
        "apiKey": "${GOOGLE_PLACES_API_KEY}"
      },
      "local-places": {
        "apiKey": "${GOOGLE_PLACES_API_KEY}"
      },
      "mcporter": {
        "enabled": true
      },
      "meeting-chief": {
        "enabled": true,
        "apiKey": "${OPENROUTER_MC_API_KEY}",
        "env": {
          "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}",
          "OPENAI_API_KEY": "${OPENAI_API_KEY}",
          "OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}",
          "OTTER_EMAIL": "${OTTER_EMAIL}",
          "OTTER_PASSWORD": "${OTTER_PASSWORD}"
        }
      },
      "nano-banana-pro": {
        "apiKey": "${GOOGLE_GEMINI_API_KEY}"
      },
      "notion": {
        "apiKey": "${NOTION_API_KEY}"
      },
      "openai-image-gen": {
        "apiKey": "${OPENAI_REALTIME_API_KEY}"
      },
      "openai-whisper-api": {
        "apiKey": "${OPENAI_REALTIME_API_KEY}"
      }
    }
  },
